package methods;

public class MethodLearning {
	//create method and call the method without invoking object.return type nothing.
	public static void studentsRank(int marks){
	
	if (marks >=600){
	System.out.println("Rank A");
	
	}
	else if(marks>=500){
		System.out.println("Rank B");
	}
	else{
		System.out.println("Rank c");
	}
	}
	
	public static void main(String[] args) {
		studentsRank(680);
		studentsRank(580);
		studentsRank(480);
		
	}
	}
